    "def check_arr_rotation(arr,n):\n",
    "    i,j = 0,0\n",
    "    min = arr[0]\n",
    "    for i in range(1,n):\n",
    "        if(min > arr[i]):\n",
    "            min = arr[i]\n",
    "            j = i\n",
    "    print(j)\n",
    "n = int(input())\n",
    "arr = [int(x) for x in input().split()]\n",
    "check_arr_rotation(arr,n)"
   ]
