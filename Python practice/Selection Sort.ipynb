   "source": [
    "# Selection Sort"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "f4a0a1cf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5 7 4 8 3\n",
      "[5, 7, 4, 8, 3]\n"
     ]
    }
   ],
   "source": [
    "arr = list(map(int,input().split()))\n",
    "length = len(arr)\n",
    "for i in range(1,length-1):\n",
    "    for j in range(i,length-1-i):\n",
    "        if(arr[i]<arr[j]):\n",
    "            arr[i],arr[j] = arr[j],arr[i]\n",
    "print(arr)"
   ]
